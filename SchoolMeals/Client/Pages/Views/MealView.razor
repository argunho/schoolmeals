@page "/schools/{municipality}/{place}/{school}/meal/{MealId:int}"


<ModelView municipality="@municipality" place="@place" school="@school" page="meal" id="@MealId"/>

@code {
    [Parameter] public int MealId { get; set; }
    [Parameter] public string municipality { get; set; }
    [Parameter] public string place { get; set; }
    [Parameter] public string school { get; set; }

}
